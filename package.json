{
  "name": "@walltowall/gatsby-starter-ww",
  "version": "1.6.15",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,ts,tsx,md,mdx}' 'gatsby-*.js' 'docs/**/*.md'",
    "clean": "gatsby clean",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "start": "yarn develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "storybook": "NODE_ENV=test start-storybook --quiet -s public -p 6006",
    "build-storybook": "NODE_ENV=test build-storybook -s public",
    "prestorybook": "yarn _copy-static-queries",
    "_copy-static-queries": "cp -r ./public/page-data/sq/d ./public/static"
  },
  "dependencies": {
    "@reach/auto-id": "^0.11.2",
    "@reach/disclosure": "^0.11.2",
    "@reach/skip-nav": "^0.11.2",
    "@reach/visually-hidden": "^0.11.1",
    "@svgr/webpack": "^5.4.0",
    "@walltowall/calico": "^0.5.0",
    "@walltowall/helpers": "^6.2.0",
    "@walltowall/react-map-slices-to-components": "^0.4.1",
    "@walltowall/siamese": "^2.1.0",
    "capsize": "^1.1.0",
    "conditional-wrap": "^1.0.2",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.8.6",
    "gatsby": "2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-catch-links": "^2.4.0",
    "gatsby-plugin-google-tagmanager": "^2.5.0",
    "gatsby-plugin-imgix": "^0.8.2",
    "gatsby-plugin-manifest": "^2.6.0",
    "gatsby-plugin-netlify": "^2.5.0",
    "gatsby-plugin-react-helmet-async": "^1.1.0",
    "gatsby-plugin-sitemap": "^2.6.0",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-treat": "^1.4.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-prismic": "^3.2.1",
    "minireset.css": "^0.0.6",
    "querystring": "^0.2.0",
    "rambdax": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-html-renderer": "^0.3.2",
    "react-map-to-components": "^1.2.1",
    "react-player": "^2.6.2",
    "react-treat": "^1.6.1",
    "treat": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.9",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/cli": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/facepaint": "^1.2.1",
    "@types/faker": "^5.1.4",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-remove-graphql-queries": "2.10.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.13.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
